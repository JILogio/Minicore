{"ast":null,"code":"import axios from 'axios';\nimport Global from '../Global';\nexport default {\n  name: 'AddAlumno',\n  mounted() {\n    this.getAlumnos();\n  },\n  data() {\n    return {\n      url: Global.url,\n      alumnos: {}\n    };\n  },\n  methods: {\n    getAlumnos() {\n      axios.get(this.url + 'Alumnos').then(res => {\n        if (res.data.status == 'success') {\n          this.alumnos = res.data.alumnos;\n        }\n      }).catch(err => {\n        console.log(err);\n      });\n    },\n    async añadirNota(id, event) {\n      event.preventDefault();\n      try {\n        let data = this.$refs['input-' + id].value;\n        console.log(data);\n      } catch (error) {\n        alert('Error al agregar nota al Alumno');\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","Global","name","mounted","getAlumnos","data","url","alumnos","methods","get","then","res","status","catch","err","console","log","añadirNota","id","event","preventDefault","$refs","value","error","alert"],"sources":["D:\\VS Code\\Minicore\\web-project\\src\\views\\AddNota.vue"],"sourcesContent":["<template>\r\n    <h1>Añadir Nota</h1>\r\n    <div class=\"center\">\r\n        <h1>Alumnos</h1>\r\n        <table>\r\n            <thead>\r\n            <tr>\r\n                <th>Nombre</th>\r\n                <th>Nota</th>\r\n                <th>Fecha</th>\r\n                <th>Acción</th>\r\n            </tr>\r\n            </thead>\r\n            <tbody>\r\n            <tr v-for=\"alumno in alumnos\" :key=\"alumno._id\">\r\n                <td>{{ alumno.nombre }}</td>\r\n                <td><input type=\"number\" :ref=\"'input-'+alumno._id\"></td>\r\n                <td><input type=\"date\" :ref=\"'input-'+alumno._id\"></td>\r\n                <td>\r\n                    <button @click=\"añadirNota(alumno._id, $event)\">Añadir Nota</button>\r\n                </td>\r\n            </tr>\r\n            </tbody>\r\n        </table>\r\n    </div>\r\n</template>\r\n\r\n<style scoped src=\"@/assets/css/AdminProductStyle.css\"></style>\r\n   \r\n<script>\r\nimport axios from 'axios';\r\nimport Global from '../Global'\r\n\r\nexport default {\r\n    name: 'AddAlumno',\r\n\r\n    mounted(){\r\n        this.getAlumnos();\r\n    },\r\n    data(){\r\n        return {\r\n            url: Global.url,\r\n            alumnos: {},\r\n        }\r\n    },\r\n    methods: {\r\n        getAlumnos(){\r\n            axios.get(this.url+'Alumnos')\r\n                .then(res => {\r\n                    if(res.data.status == 'success'){\r\n                        this.alumnos = res.data.alumnos;\r\n                    }\r\n                })\r\n                .catch (err => {\r\n                    console.log(err)\r\n                })\r\n        },\r\n        async añadirNota(id,event) {\r\n            event.preventDefault();\r\n            try {\r\n                let data = this.$refs['input-'+id].value;\r\n                console.log(data)\r\n            } catch(error) {\r\n                alert('Error al agregar nota al Alumno');\r\n            }\r\n        }\r\n    }\r\n};\r\n</script>"],"mappings":"AA8BA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,MAAK,MAAO,WAAU;AAE7B,eAAe;EACXC,IAAI,EAAE,WAAW;EAEjBC,OAAOA,CAAA,EAAE;IACL,IAAI,CAACC,UAAU,CAAC,CAAC;EACrB,CAAC;EACDC,IAAIA,CAAA,EAAE;IACF,OAAO;MACHC,GAAG,EAAEL,MAAM,CAACK,GAAG;MACfC,OAAO,EAAE,CAAC;IACd;EACJ,CAAC;EACDC,OAAO,EAAE;IACLJ,UAAUA,CAAA,EAAE;MACRJ,KAAK,CAACS,GAAG,CAAC,IAAI,CAACH,GAAG,GAAC,SAAS,EACvBI,IAAI,CAACC,GAAE,IAAK;QACT,IAAGA,GAAG,CAACN,IAAI,CAACO,MAAK,IAAK,SAAS,EAAC;UAC5B,IAAI,CAACL,OAAM,GAAII,GAAG,CAACN,IAAI,CAACE,OAAO;QACnC;MACJ,CAAC,EACAM,KAAI,CAAGC,GAAE,IAAK;QACXC,OAAO,CAACC,GAAG,CAACF,GAAG;MACnB,CAAC;IACT,CAAC;IACD,MAAMG,UAAUA,CAACC,EAAE,EAACC,KAAK,EAAE;MACvBA,KAAK,CAACC,cAAc,CAAC,CAAC;MACtB,IAAI;QACA,IAAIf,IAAG,GAAI,IAAI,CAACgB,KAAK,CAAC,QAAQ,GAACH,EAAE,CAAC,CAACI,KAAK;QACxCP,OAAO,CAACC,GAAG,CAACX,IAAI;MACpB,EAAE,OAAMkB,KAAK,EAAE;QACXC,KAAK,CAAC,iCAAiC,CAAC;MAC5C;IACJ;EACJ;AACJ,CAAC"},"metadata":{},"s