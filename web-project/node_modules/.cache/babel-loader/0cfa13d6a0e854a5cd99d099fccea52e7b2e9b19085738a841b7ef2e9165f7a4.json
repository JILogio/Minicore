{"ast":null,"code":"import axios from 'axios';\nimport Global from '../Global';\nexport default {\n  name: 'AlumnosPage',\n  mounted() {\n    this.getAlumnos();\n    this.getNotas();\n  },\n  data() {\n    return {\n      url: Global.url,\n      alumnos: {},\n      notas: {}\n    };\n  },\n  methods: {\n    getAlumnos() {\n      axios.get(this.url + 'Alumnos').then(res => {\n        if (res.data.status == 'success') {\n          this.alumnos = res.data.alumnos;\n        }\n      }).catch(err => {\n        console.log(err);\n      });\n    },\n    getNotas() {\n      axios.get(this.url + 'Notas').then(res => {\n        if (res.data.status == 'success') {\n          this.notas = res.data.notas;\n        }\n      }).catch(err => {\n        console.log(err);\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","Global","name","mounted","getAlumnos","getNotas","data","url","alumnos","notas","methods","get","then","res","status","catch","err","console","log"],"sources":["D:\\VS Code\\Minicore\\web-project\\src\\views\\NotasPage.vue"],"sourcesContent":["<template>\r\n    <div class=\"center\">\r\n        <h1>Alumnos</h1>\r\n        <table>\r\n            <thead>\r\n            <tr>\r\n                <th>Nombre</th>\r\n            </tr>\r\n            </thead>\r\n            <tbody>\r\n            <tr v-for=\"alumno in alumnos\" :key=\"alumno._id\">\r\n                <td>{{ alumno.nombre }}</td>\r\n                <td>\r\n                    <span v-for=\"nota in notas\" :key=\"nota._id\" v-if=\"this.nota.idAlumno === alumno._id\">\r\n                    {{ nota.nota }}\r\n                    </span>\r\n                </td>\r\n            </tr>\r\n            </tbody>\r\n        </table>\r\n    </div>\r\n</template>\r\n\r\n<style scoped src=\"@/assets/css/AdminProductStyle.css\"></style>\r\n\r\n<script>\r\nimport axios from 'axios';\r\nimport Global from '../Global'\r\n\r\nexport default {\r\n    name: 'AlumnosPage',\r\n\r\n    mounted(){\r\n        this.getAlumnos();\r\n        this.getNotas();\r\n    },\r\n    data(){\r\n        return {\r\n            url: Global.url,\r\n            alumnos: {},\r\n            notas: {},\r\n        }\r\n    },\r\n    methods: {\r\n        getAlumnos(){\r\n            axios.get(this.url+'Alumnos')\r\n                .then(res => {\r\n                    if(res.data.status == 'success'){\r\n                        this.alumnos = res.data.alumnos;\r\n                    }\r\n                })\r\n                .catch (err => {\r\n                    console.log(err)\r\n                })\r\n        },\r\n        getNotas(){\r\n            axios.get(this.url+'Notas')\r\n                .then(res => {\r\n                    if(res.data.status == 'success'){\r\n                        this.notas = res.data.notas;\r\n                    }\r\n                })\r\n                .catch (err => {\r\n                    console.log(err)\r\n                })\r\n        }\r\n    }\r\n}\r\n</script>"],"mappings":"AA0BA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,MAAK,MAAO,WAAU;AAE7B,eAAe;EACXC,IAAI,EAAE,aAAa;EAEnBC,OAAOA,CAAA,EAAE;IACL,IAAI,CAACC,UAAU,CAAC,CAAC;IACjB,IAAI,CAACC,QAAQ,CAAC,CAAC;EACnB,CAAC;EACDC,IAAIA,CAAA,EAAE;IACF,OAAO;MACHC,GAAG,EAAEN,MAAM,CAACM,GAAG;MACfC,OAAO,EAAE,CAAC,CAAC;MACXC,KAAK,EAAE,CAAC;IACZ;EACJ,CAAC;EACDC,OAAO,EAAE;IACLN,UAAUA,CAAA,EAAE;MACRJ,KAAK,CAACW,GAAG,CAAC,IAAI,CAACJ,GAAG,GAAC,SAAS,EACvBK,IAAI,CAACC,GAAE,IAAK;QACT,IAAGA,GAAG,CAACP,IAAI,CAACQ,MAAK,IAAK,SAAS,EAAC;UAC5B,IAAI,CAACN,OAAM,GAAIK,GAAG,CAACP,IAAI,CAACE,OAAO;QACnC;MACJ,CAAC,EACAO,KAAI,CAAGC,GAAE,IAAK;QACXC,OAAO,CAACC,GAAG,CAACF,GAAG;MACnB,CAAC;IACT,CAAC;IACDX,QAAQA,CAAA,EAAE;MACNL,KAAK,CAACW,GAAG,CAAC,IAAI,CAACJ,GAAG,GAAC,OAAO,EACrBK,IAAI,CAACC,GAAE,IAAK;QACT,IAAGA,GAAG,CAACP,IAAI,CAACQ,MAAK,IAAK,SAAS,EAAC;UAC5B,IAAI,CAACL,KAAI,GAAII,GAAG,CAACP,IAAI,CAACG,KAAK;QAC/B;MACJ,CAAC,EACAM,KAAI,CAAGC,GAAE,IAAK;QACXC,OAAO,CAACC,GAAG,CAACF,GAAG;MACnB,CAAC;IACT;EACJ;AACJ"},"metadata":{},"sourceType":"module","externalDepen